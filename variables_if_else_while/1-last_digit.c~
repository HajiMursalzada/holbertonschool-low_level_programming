#include <stdlib.h>
#include <time.h>
#include <stdio.h>
/* more headers goes there */

/* betty style doc for function main goes there */
int lastDigit(int number){
    int lastDigit = number % 10;
    return lastDigit;
}

int main(void)
{
	int n;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	/* your code goes there */
	last_digit = lastDigit(number);
	if (n > 5)
        {
                printf ("Last digit of %d is %d %s\n", n, number, last_digit,"and is greater than 5");
        }
	else if ( 6 > n > 0 )
        {
                printf ("Last digit of %d is %d %s\n", n, number, last_digit,"and is less than 6 and not 0");
        }
        else if (n = 0)
        {
                printf ("Last digit of %d is %d %s\n", n, number, last_digit,"and is 0");
        }
	return (0);
}
